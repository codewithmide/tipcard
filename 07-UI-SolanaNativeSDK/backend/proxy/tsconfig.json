{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "tests",
    "**/*spec.ts",
    "jest.config.ts"
  ]
}
